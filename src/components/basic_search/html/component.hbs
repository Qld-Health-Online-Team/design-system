<!--@@ Banner @@-->
<section class="qld__banner qld__banner--search
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'light'}}qld__banner--light{{/ifCond}} 
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'alternate'}}qld__banner--alt{{/ifCond}} 
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark'}}qld__banner--dark{{/ifCond}} 
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark-alternate'}}qld__banner--dark-alt{{/ifCond}} 

        {{#ifCond current.data.metadata.displayBreadcrumbs.value '==' 'true'}}qld__banner--breadcrumbs{{/ifCond}}"
        
        {{#ifCond site.metadata.defaultBannerType.value '==' 'texture'}}
        style="background-image: url(./?a={{site.metadata.defaultBannerTexture.value}});"
        {{/ifCond}}
>
    <!--@@ Breadcrumbs - Mobile @@-->
    {{#ifCond current.data.metadata.displayBreadcrumbs.value  '==' 'true'}}
    <nav class="qld__breadcrumbs
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'light'}}qld__breadcrumbs--light{{/ifCond}}
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'alternate'}}qld__breadcrumbs--alt{{/ifCond}}
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark'}}qld__breadcrumbs--dark{{/ifCond}}
        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark-alternate'}}qld__breadcrumbs--dark-alt{{/ifCond}}
        
        qld__banner__breadcrumbs qld__banner__breadcrumbs--mobile" aria-label="breadcrumb"> 
        <div class="qld__link-list qld__link-list--inline">
            {{#with (itemAt current.lineage -2)}}
                {{#ifCond asset_type_code '!=' 'folder'}} 
                    <div class="qld__breadcrumbs__item">
                        <a href="{{asset_url}}" class="qld__breadcrumbs__link">{{#ifCond asset_is_site_asset '==' '1'}}Home{{else}}{{asset_short_name}}{{/ifCond}}</a>    
                    </div>
                {{/ifCond}}
            {{/with}}
        </div>
    </nav>
    {{/ifCond}}

    <div class="container-fluid">
        <div class="qld__banner__wrapper">
            
            <div class="qld__banner__main row">              
                
                <div class="qld__banner__content col-xs-12 col-md-12 col-lg-4">
                    <!--@@ Breadcrumbs - Tablet @@-->
                    {{#ifCond current.data.metadata.displayBreadcrumbs.value  '==' 'true'}}
                    <nav class="qld__breadcrumbs
                        {{#ifCond site.metadata.defaultBannerColour.value '==' 'light'}}qld__breadcrumbs--light{{/ifCond}}
                        {{#ifCond site.metadata.defaultBannerColour.value '==' 'alternate'}}qld__breadcrumbs--alt{{/ifCond}}
                        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark'}}qld__breadcrumbs--dark{{/ifCond}}
                        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark-alternate'}}qld__breadcrumbs--dark-alt{{/ifCond}}
                        
                        qld__banner__breadcrumbs qld__banner__breadcrumbs--tablet" aria-label="breadcrumb" data-path="{{@root.site.metadata.coreSiteIcons.value}}"> 
                        <div class="qld__link-list qld__link-list--inline">
                            {{#with (itemAt current.lineage -2)}}
                                {{#ifCond asset_type_code '!=' 'folder'}} 
                                    <div class="qld__breadcrumbs__item">
                                        <a href="{{asset_url}}" class="qld__breadcrumbs__link">{{#ifCond asset_is_site_asset '==' '1'}}Home{{else}}{{asset_short_name}}{{/ifCond}}</a>    
                                    </div>
                                {{/ifCond}}
                            {{/with}}
                        </div>
                    </nav>
                    {{/ifCond}}
                    <!--@@ Breadcrumbs - Desktop @@-->
                    {{#ifCond current.data.metadata.displayBreadcrumbs.value '==' 'true'}}
                        <nav class="qld__breadcrumbs 
                        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark'}}qld__breadcrumbs--dark{{/ifCond}} 
                        {{#ifCond site.metadata.defaultBannerColour.value '==' 'dark-alternate'}}qld__breadcrumbs--dark{{/ifCond}} 
                        qld__banner__breadcrumbs qld__banner__breadcrumbs--desktop" aria-label="breadcrumb" data-path="{{@root.site.metadata.coreSiteIcons.value}}">
                            <ol class="qld__link-list qld__link-list--inline">
                                {{#each current.lineage}}   
                                {{#ifCond asset_type_code '!=' 'folder'}}      
                                    <li {{#if @last}}aria-current="page"{{/if}} class="qld__breadcrumbs__item">
                                        {{#if @last}}
                                            {{asset_short_name}}
                                        {{else}}
                                        <a href="{{asset_url}}" class="qld__breadcrumbs__link">{{#if @first}}Home{{else}}{{asset_short_name}}{{/if}}</a>    
                                        {{/if}}
                                    </li>
                                {{/ifCond}}
                                {{/each}}
                            </ol>
                        </nav>
                    {{/ifCond}}

                    <!--@@ Heading(s) @@-->
                    <h1>Search Results</h1>

                    <!--@@ Abstract @@-->
                    {{#if metadata.abstract.value}}
                        <div class="qld__banner__content--body">{{{metadata.abstract.value}}}</div>
                    {{/if}}
                    
                </div>
                <div class="qld__banner__search col-xs-12 col-md-12 col-lg-8">
                    <div class="qld__search-form--wrapper">
                        <form role="search" aria-label="sitewide" class="qld__search-form" id="search-form-global-basic" action="{{site.metadata.siteSearchUrl.value}}">
                            <div class="qld__search-form__inner">
                                <svg class="qld__icon qld__icon--sm qld__search-form__inner-icon" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"><use href="{{@root.site.metadata.coreSiteIcons.value}}#search"></use></svg>
                                <input type="search" id="search-input-global-basic-query" name="query" class="qld__text-input data-hj-allow" aria-expanded="false" autocomplete="off" disabled="true" value="{{#ifCond resultsPage.question.query '!==' '!padrenull'}}{{resultsPage.question.query}}{{/ifCond}}">
                                <input type="text" id="name" name="name" autocomplete="off" tabindex="-1" class="qld__text-input--validation">
                                <div class="qld__search-form__btn">
                                    <button class="qld__btn qld__btn--search" type="button" aria-label="Search" disabled="true">
                                        <span class="qld__btn__icon">
                                            <svg class="qld__icon" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"><use href="{{@root.site.metadata.coreSiteIcons.value}}#search"></use></svg>
                                        </span>
                                        <span class="qld__btn__text">Search</span>
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="qld__body qld__search" id="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12 col-md-12">
                <div class="qld__search__info">
                    <div class="qld__search__info-query">
                        <h2 class="qld__display-lg">Results{{#ifCond resultsPage.question.query '!==' ''}} for <span class="qld__search__info-query-term">‘{{resultsPage.question.query}}’</span>{{/ifCond}}</h2>
                    </div>
                    <div class="qld__search__info-inner">
                        <div class="qld__search__info-summary">
                            <span class="qld__search__info-summary-text">Showing:</span> 
                            <span class="qld__search__info-summary-start-end">{{resultsPage.resultsSummary.currStart}} - {{resultsPage.resultsSummary.currEnd}} of</span>
                            <span class="qld__search__info-summary-matching">{{resultsPage.resultsSummary.totalMatching}} results</span>   
                        </div>

                        <div class="qld__search__sort">
                            <label for="search-sort" class="qld__search__sort-text">Sort by:</label>
                            <select id="search-sort" class="qld__select-control" name="sort">
                                <option value="" selected="">Relevance</option>
                                <option value="metafileName">A-Z</option>   
                                <option value="dmetafileName">Z-A</option>   
                                <option value="date">Newest</option>
                                <option value="adate">Oldest</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="qld__search__results">
                    {{#if resultsPage.results}}
                    <ol class="qld__link-list qld__search__results-list">
                        {{#each resultsPage.results}}
                        <li class="qld__search__result">
                            <div class="qld__search__result-inner">
                                <div class="qld__search__result-header">
                                    {{#ifCond collection 'contains' 'service-finder'}}
                                        {{#ifCond collection 'contains' '__service-locations'}}
                                            <h3 class="qld__display-md">
                                                <a href="{{@root.site.metadata.sfHomeUrl.value}}/{{listMetadata.servicewebpath}}">
                                                    Healthcare services | {{{title}}}
                                                </a>
                                            </h3>
                                        {{else ifCond collection 'contains' '__facilities'}}
                                            <h3 class="qld__display-md">
                                                <a href="{{@root.site.metadata.sfFacilitiesUrl.value}}/{{listMetadata.webpath}}">
                                                    Healthcare services | {{{title}}}
                                                </a>
                                            </h3>
                                        {{else}} 
                                            <h3 class="qld__display-md">
                                                <a href="{{@root.site.metadata.sfHomeUrl.value}}/{{listMetadata.webpath}}">
                                                    Healthcare services | {{{title}}}
                                                </a>
                                            </h3>
                                        {{/ifCond}}
                                    {{else}}
                                        <h3 class="qld__display-md">    
                                            <a href="{{liveUrl}}">
                                                {{#ifCond liveUrl 'contains' '?tag='}}
                                                    {{replace title '\|.*' ''}} | Topic | {{#with (split liveUrl '?tag=') as | tagUrl | }}{{#replaceMany "%27" "'"}} {{#replaceMany "_" " "}}{{capitaliseFirst tagUrl.[1]}}{{/replaceMany}} {{/replaceMany}}{{/with}}
                                                {{else ifCond fileType '==' 'pdf'}}
                                                    {{replace listMetadata.fileName.[0] '\|.*' ''}}
                                                {{else}}
                                                    {{replace title '\|.*' ''}}
                                                {{/ifCond}}
                                            </a>
                                        </h3>
                                    {{/ifCond}}
                                    {{#if listMetadata.assetLineage}}
                                    <div class="qld__breadcrumbs qld__search__result-breadcrumbs" aria-label="breadcrumb">
                                        <ul class="qld__link-list qld__link-list--inline">
                                            {{#each (split listMetadata.assetLineage "+")}}
                                                <li>{{this}}</li>
                                            {{/each}}       
                                        </ul>
                                    </div>
                                    {{/if}}
                                    {{#ifCond collection 'contains' '__service-locations'}}
                                    <div class="qld__breadcrumbs qld__search__result-breadcrumbs" aria-label="breadcrumb">
                                        <ul class="qld__link-list qld__link-list--inline">
                                            <li>Services</li>
                                            <li>{{itemAt (split title '-') '0' }}</li>
                                            <li>{{{title}}}</li>
                                        </ul>
                                    </div>
                                    {{/ifCond}}
                                    {{#ifCond collection 'contains' '__facilities'}}
                                    <div class="qld__breadcrumbs qld__search__result-breadcrumbs" aria-label="breadcrumb">
                                        <ul class="qld__link-list qld__link-list--inline">
                                            <li>Services</li>
                                            <li>Locations</li>
                                            <li>{{{title}}}</li>
                                        </ul>
                                    </div>
                                    {{/ifCond}}
                                    {{#ifCond collection 'contains' '__services'}}
                                    <div class="qld__breadcrumbs qld__search__result-breadcrumbs" aria-label="breadcrumb">
                                        <ul class="qld__link-list qld__link-list--inline">
                                            <li>Services</li>
                                            <li>{{{title}}}</li>
                                        </ul>
                                    </div>
                                    {{/ifCond}}
                                </div> 
                                <div class="qld__search__result-type">
                                {{#if listMetadata.articleType}}
                                    {{#ifCond listMetadata.articleType '==' 'news'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#article"></use></svg>
                                            News
                                        </span>
                                        {{/ifCond}}
                                    {{#ifCond listMetadata.articleType '==' 'feature-article'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#announcement"></use></svg>
                                            Feature Article
                                        </span>
                                        {{/ifCond}}
                                    {{#ifCond listMetadata.articleType '==' 'podcast-series'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#podcast"></use></svg>
                                            Podcast
                                        </span>
                                    {{/ifCond}}
                                {{/if}}
                                {{#if fileType}}
                                    {{#ifCond fileType '==' 'pdf'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#document-pdf"></use></svg>
                                            Document
                                        </span>
                                        <span class="qld__tag qld__tag--info">{{toUpperCase fileType}} {{sizeFormat fileSize}}</span>
                                    {{/ifCond}}      
                                    {{#ifCond fileType '==' 'docx'}}
                                            <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#document-word"></use></svg>
                                            Document
                                        </span>
                                        <span class="qld__tag qld__tag--info">{{toUpperCase fileType}} {{sizeFormat fileSize}}</span>
                                    {{/ifCond}} 
                                    {{#ifCond fileType '==' 'xls'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#document-spreadsheet"></use></svg>
                                            Document
                                        </span>
                                        <span class="qld__tag qld__tag--info">{{toUpperCase fileType}} {{sizeFormat fileSize}}</span>
                                    {{/ifCond}}          
                                {{/if}}
                                {{#ifCond collection 'contains' 'service-finder'}}
                                    {{#ifCond collection 'contains' '__facilities'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.extendedSiteIcons.value}}#hospital"></use></svg>
                                            Hospital
                                        </span>
                                    {{else}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.extendedSiteIcons.value}}#health_services"></use></svg>
                                            Health Service
                                        </span>
                                    {{/ifCond}}
                                {{/ifCond}}
                                {{#if metaData.format}}
                                    {{#ifCond metaData.format 'contains' 'video'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#video"></use></svg>
                                            Video
                                        </span>
                                    {{else ifCond metaData.format 'contains' 'audio'}}
                                        <span class="qld__search__result-type-name">
                                            <svg class="qld__icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" ><use href="{{@root.site.metadata.coreSiteIcons.value}}#audio"></use></svg>
                                            Audio
                                        </span>
                                    {{/ifCond}}
                                {{/if}}                                
                                </div>
                            </div>
                            <p class="qld__search__result-summary">
                                {{#ifCond collection 'contains' '__facilities'}}
                                    {{#if metaData.building}}{{{metaData.building}}}, {{/if}}{{#if metaData.physicalAddressStreet}}{{{metaData.physicalAddressStreet}}}{{/if}}{{#if metaData.physicalAddressSuburb}}, {{{metaData.physicalAddressSuburb}}}{{/if}}{{#if metaData.physicalAddressPostcode}}, {{{metaData.physicalAddressPostcode}}}{{/if}}
                                {{else}}
                                    {{#ifCond collection 'contains' '__service-locations'}}
                                        {{#if metaData.building}}{{{metaData.building}}}, {{/if}}{{#if metaData.physicalAddressStreet}}{{{metaData.physicalAddressStreet}}}{{/if}}{{#if metaData.physicalAddressSuburb}}, {{{metaData.physicalAddressSuburb}}}{{/if}}{{#if metaData.physicalAddressPostcode}}, {{{metaData.physicalAddressPostcode}}}{{/if}}
                                    {{else}}
                                        {{#if metaData.d}}<span class="qld__search__result-date">{{formatDate listMetadata.d.[0] 'j F Y'}} —</span>{{/if}}
                                        {{{charMax summary '160'}}}
                                    {{/ifCond}}
                                {{/ifCond}}
                            </p>                           
                        </li>
                        {{/each}}
                    </ol>
                    {{/if}}
                </div>
                

                {{#ifCond resultsPage.resultsSummary.totalMatching '<' '1'}}
                <div class="qld__search__results qld__search__results--no-results">
                    <div class="qld__abstract">
                        Did you mean <a href="{{spell.url}}">‘{{{spell.text}}}’</a>
                    </div>
                    <h2 class="qld__display-lg"><span>Our search for </span><span class="qld__search__info-query-term">‘{{{question.query}}}’</span> didn't return any results.</h2>
                    <p class="qld__abstract">You could try:</p>
                    <ul class="qld__search-link-list qld__link-list">
                        <li>Check the spelling of your keywords.</li>
                        <li>Searching again using other words</li>
                        <li>Using fewer keywords.</li>
                    </ul>
                </div>
                {{/ifCond}}
                

                {{#ifCond resultsPage.resultsSummary.totalMatching '>=' '11'}}
                <nav class="text-center hidden-print qld__search-pagination" aria-label="pagination">
                    <h2 class="sr-only">Pagination</h2> 
                    <ul class="qld__search-pagination__list">
                        {{#each resultsPage.pagination}}
                            {{#ifCond linkType '===' 'last'}}
                                <li class="ellipsis">
                                    <svg class="qld__icon qld__icon--lg" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"><use href="{{@root.site.metadata.coreSiteIcons.value}}#more-horizontal"></use></svg>
                                </li>
                            {{/ifCond}}

                            
                                {{#ifCond linkType '===' 'prev'}}
                                <li class="prev">
                                    <a href="{{{url}}}" rel="prev" aria-label="Previous page of results">
                                        <svg class="qld__icon qld__icon--sm" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"><use href="{{@root.site.metadata.coreSiteIcons.value}}#arrow-left"></use></svg>
                                        <span>Back</span>
                                    </a>
                                {{/ifCond}}
                                
                                {{#if label}}
                                <li class="{{linkType}}{{#if isCurrent}} active{{/if}}">
                                    <a href="{{{url}}}" aria-current="page">{{label}}</a>
                                </li>
                                {{/if}}

                                {{#ifCond linkType '===' 'next'}}
                                <li class="next">
                                    <a href="{{{url}}}" rel="next" aria-label="Next page of results">
                                        <span>Next</span>
                                        <svg class="qld__icon qld__icon--sm" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"><use href="{{@root.site.metadata.coreSiteIcons.value}}#arrow-right"></use></svg>
                                    </a>
                                {{/ifCond}}
                            </li>
                        
                            {{#ifCond linkType '===' 'first'}}
                                <li class="ellipsis">
                                    <svg class="qld__icon qld__icon--sm" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"><use href="{{@root.site.metadata.coreSiteIcons.value}}#more-horizontal"></use></svg>
                                </li>
                            {{/ifCond}}
                        {{/each}}
                    </ul>
                </nav>
                {{/ifCond}}            
            </div>
        </div>
    </div>
</section>